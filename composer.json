        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "genealabs/bones-marshal": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1@dev",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/seeds"
        ],
        "psr-4": {
            "GeneaLabs\\Bones\\Keeper\\": "src/GeneaLabs/Bones/Keeper"
        }
    },
    "minimum-stability": "stable"
}
