{
    "name": "genealabs/bones-keeper",
    "description": "Authentication and ACL functionality for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "info@genealabs.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "require-dev": {
        "phpspec/phpspec": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/seeds"
        ],
        "psr-4": {
            "GeneaLabs\\Bones\\Keeper\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan migrate --package='genealabs/bones-keeper'",
            "php artisan db:seed --class='BonesKeeperDatabaseSeeder'",
            "php artisan asset:publish genealabs/boneskeeper"
        ],
        "post-update-cmd": [
            "php artisan migrate --package='genealabs/bones-keeper'",
            "php artisan asset:publish genealabs/boneskeeper"
        ]
    },
    "minimum-stability": "stable"
}
