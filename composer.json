{
  "name": "genealabs/laravel-governor",
  "description": "Managing policy and control in Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "GeneaLabs, LLC",
      "email": "hello@genealabs.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "genealabs/laravel-casts": "*",
    "illuminate/support": "5.0 - 5.6"
  },
  "require-dev": {
      "codedungeon/phpunit-result-printer": "*",
      "doctrine/dbal": "^2.5",
      "fzaninotto/faker": "~1.4",
      "mockery/mockery": "0.9.*",
      "orchestra/database": "3.6.x-dev@dev",
      "orchestra/testbench-browser-kit": "^3.6",
      "orchestra/testbench-dusk": "3.6.x-dev@dev",
      "orchestra/testbench": "^3.6",
      "php-coveralls/php-coveralls" : "*",
      "phpmd/phpmd": "*",
      "phpunit/phpunit": "*",
      "sebastian/phpcpd": "*",
      "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/migrations",
      "database/seeds"
    ],
    "psr-4": {
      "GeneaLabs\\LaravelGovernor\\": "src/",
      "GeneaLabs\\LaravelGovernor\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "GeneaLabs\\LaravelGovernor\\Providers\\Service"
          ]
      }
  },
  "config": {
    "preferred-install": "dist"
  }
}
