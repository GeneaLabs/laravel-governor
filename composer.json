{
  "name": "genealabs/laravel-governor",
  "description": "Managing policy and control in Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "GeneaLabs, LLC",
      "email": "hello@genealabs.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "genealabs/laravel-casts": "*",
    "illuminate/support": "5.5 - 5.6"
  },
  "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "laravel/laravel": "5.6.*",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "*",
        "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/migrations",
      "database/seeds"
    ],
    "psr-4": {
      "GeneaLabs\\LaravelGovernor\\": "src/",
      "GeneaLabs\\LaravelGovernor\\Tests\\": "tests/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "GeneaLabs\\LaravelGovernor\\Providers\\Service"
          ]
      }
  },
  "config": {
    "preferred-install": "dist"
  }
}
